    <color name="yellow">#FFFF00</color>
    <declare-styleable name="FlowLayout"><attr format="dimension" name="horizontalSpacing"/><attr format="dimension" name="verticalSpacing"/><attr format="enum" name="orientation">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr><attr format="boolean" name="debugDraw"/></declare-styleable>
    <declare-styleable name="FlowLayout_LayoutParams"><attr format="boolean" name="layout_newLine"/><attr format="dimension" name="layout_horizontalSpacing"/><attr format="dimension" name="layout_verticalSpacing"/></declare-styleable>
    <declare-styleable name="SDRatingBar"><attr format="reference" name="progressDrawable"/></declare-styleable>
    <declare-styleable name="SlidingDrawer"><attr name="s_orientation">
    
        　　            <enum name="horizontal" value="0"/>
    
        　　            <enum name="vertical" value="1"/>
    
    　　</attr><attr format="reference" name="s_content"/><attr format="reference" name="s_handle"/><attr format="dimension" name="s_bottomOffset"/><attr format="dimension" name="s_topOffset"/><attr format="boolean" name="s_allowSingleTap"/><attr format="boolean" name="s_animateOnClick"/></declare-styleable>
    <dimen name="height_dialog_item">50dp</dimen>
    <dimen name="height_dialog_progress">70dp</dimen>
    <dimen name="text_size_button">16sp</dimen>
    <dimen name="text_size_content">15sp</dimen>
    <dimen name="text_size_title">20sp</dimen>
    <dimen name="width_min_title_item">45dp</dimen>
    <string name="action_settings">Settings</string>
    <string name="app_name">library_iosDialog</string>
    <string name="hello_world">Hello world!</string>
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="anim_bottom_bottom" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/dialog_enter_sliding_from_bottom</item>
        <item name="@android:windowExitAnimation">@anim/dialog_exit_sliding_to_bottom</item>
    </style>
    <style name="anim_right_right" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/dialog_enter_sliding_from_right</item>
        <item name="@android:windowExitAnimation">@anim/dialog_exit_sliding_to_right</item>
    </style>
    <style name="anim_top_top" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/dialog_enter_sliding_from_top</item>
        <item name="@android:windowExitAnimation">@anim/dialog_exit_sliding_to_top</item>
    </style>
    <style name="dialogBase" parent="@android:style/Theme.Dialog">

        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 背景透明 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 半透明 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 模糊 -->
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="dialogBaseBlur" parent="dialogBase">

        <!-- 模糊 -->
        <item name="android:backgroundDimEnabled">true</item>
    </style>
</resources>